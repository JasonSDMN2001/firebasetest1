<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeFragment"
    >

    <!-- TODO: Update blank fragment layout -->


    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/profile_button2"
        android:layout_width="86dp"
        android:layout_height="86dp"
        android:layout_marginTop="56dp"
        android:background="?android:selectableItemBackground"
        android:backgroundTint="#9D9E9E"
        android:onClick="profileChange"
        android:scaleType="centerCrop"
        android:padding="3dp"
        android:src="@drawable/baby1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/profile_button3"
        app:layout_constraintTop_toTopOf="parent"
        app:shapeAppearanceOverlay="@style/Circle"
        app:strokeColor="#2ECFB0"
        app:strokeWidth="5dp"
        tools:ignore="SpeakableTextPresentCheck,ImageContrastCheck" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/profile_button3"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginStart="252dp"
        android:layout_marginTop="56dp"
        android:background="?android:selectableItemBackground"
        android:backgroundTint="#9D9E9E"
        android:onClick="profileChangeBaby"
        android:scaleType="centerCrop"
        android:src="@drawable/baby1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:shapeAppearanceOverlay="@style/Circle"
        app:strokeColor="#2ECFB0"
        app:strokeWidth="5dp"
        android:padding="3dp"
        tools:ignore="SpeakableTextPresentCheck,ImageContrastCheck" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/fraunces_bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>